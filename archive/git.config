[user]
	name = Krys Flores
	email = krystal.flores@gmail.com
[core]
    excludesfile = /Users/krystalflores/.gitignore_global
[push]
   autoSetupRemote = true
[alias]
	ci = commit -am
	w = diff -M -w --color-words
	fa = fetch --all --tags
	v = "!f() {git tag --sort=v:refname | tail -10; }; f"
	st = status
	ps = push
	pl = pull
[diff]
	color = auto
	rename = copy
	ignore-space-at-eol = true
	b = true
	w = true
	ignore-blank-lines = true
	algorithm = histogram
	renameLimit = 200000
[status]
	color = auto
